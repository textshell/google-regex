{
  "manifest_version": 2,

  "name": "Google regex",
  "description": "Filter search results by site.",
  "version": "0.5",

  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["event.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["https://www.google.de/*"],
      "js": ["content.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
